<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record model = "ir.ui.view" id = "point_tree_view">
        <field name = "name">custom.model.point.tree</field>
        <field name = "model">custom.model.point</field>
        <field name = "arch" type = "xml">
            <tree create="0" delete="false">
                <field name = "employee_id"/>
                <field name = "number"/>
            </tree>
        </field>
    </record>

    <record model = "ir.ui.view" id = "archive_point_tree_view">
        <field name = "name">archive.point.tree</field>
        <field name = "model">archive.point</field>
        <field name = "arch" type = "xml">
            <tree create="0" delete="false">
                <field name = "employee_id"/>
                <field name = "state"/>
                <field name = "number"/>
            </tree>
        </field>
    </record>

</odoo>